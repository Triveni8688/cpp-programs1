#include <iostream>
using namespace std;

int main()
{
     double firstnumber, secondnumber, productoftwonumbers;
     cout << "Enter two numbers: ";
     
     cin >> firstnumber >> secondnumber;
     
     productoftwonumbers = firstnumber * secondnumber;
     
     cout << "Product = " << productoftwonumbers;
     
     return 0;
}
